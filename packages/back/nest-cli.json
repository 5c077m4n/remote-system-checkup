{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway-service/src",
  "monorepo": true,
  "root": "apps/gateway-service",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/gateway-service/tsconfig.app.json"
  },
  "projects": {
    "gateway-service": {
      "type": "application",
      "root": "apps/gateway-service",
      "entryFile": "main",
      "sourceRoot": "apps/gateway-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway-service/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "gql-service": {
      "type": "application",
      "root": "apps/gql-service",
      "entryFile": "main",
      "sourceRoot": "apps/gql-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gql-service/tsconfig.app.json"
      }
    },
    "sensor-service": {
      "type": "application",
      "root": "apps/sensor-service",
      "entryFile": "main",
      "sourceRoot": "apps/sensor-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sensor-service/tsconfig.app.json"
      }
    }
  }
}