{
	"name": "remote-system-checkup-back",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"license": "MIT",
	"scripts": {
		"rust:run": "(cd ../encryption && cargo run)",
		"rust:watch": "(cd ../encryption && cargo watch -x run)",
		"rust:debug": "(cd ../encryption && cargo watch -x clippy)",
		"prebuild": "rimraf dist",
		"build": "nest build --webpack",
		"format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
		"start": "nest start --webpack & run-s rust:run",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"watch:gateway": "npm run start -- gateway-service",
		"watch:auth": "npm run start -- auth-service",
		"watch:sensor": "npm run start -- sensor-service",
		"watch:gql": "npm run start -- gql-service",
		"watch:encrypt": "npm run rust:watch",
		"watch": "run-p watch:*",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./apps/gateway-service/test/jest-e2e.json",
		"clean": "rimraf ./node_modules"
	},
	"dependencies": {
		"@godaddy/terminus": "^4.3.1",
		"@nestjs/common": "^6.7.2",
		"@nestjs/core": "^6.7.2",
		"@nestjs/graphql": "^6.5.3",
		"@nestjs/microservices": "^6.10.14",
		"@nestjs/platform-express": "^6.7.2",
		"@nestjs/platform-fastify": "^6.10.14",
		"@nestjs/terminus": "^6.5.5",
		"amqp-connection-manager": "^3.1.1",
		"amqplib": "^0.5.5",
		"apollo-server-express": "^2.9.16",
		"graphql": "^14.5.8",
		"graphql-tools": "^4.0.6",
		"helmet": "^3.21.2",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.0",
		"rxjs": "^6.5.3",
		"systeminformation": "^4.18.3",
		"webpack-node-externals": "^1.7.2"
	},
	"devDependencies": {
		"@nestjs/cli": "^6.13.3",
		"@nestjs/schematics": "^6.8.2",
		"@nestjs/testing": "^6.7.1",
		"@types/express": "^4.17.1",
		"@types/jest": "^24.0.18",
		"@types/node": "^12.7.5",
		"@types/supertest": "^2.0.8",
		"jest": "^24.9.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.18.2",
		"supertest": "^4.0.2",
		"ts-jest": "^24.3.0",
		"ts-loader": "^6.1.1",
		"ts-node": "^8.6.1",
		"tsconfig-paths": "^3.9.0",
		"tslint": "^5.20.0",
		"typescript": "^3.6.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": ".",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "./coverage",
		"testEnvironment": "node",
		"roots": [
			"<rootDir>/apps/"
		]
	}
}
